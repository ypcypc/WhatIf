      },
      "gemini": {
        "models": ["gemini-2.5-pro", "gemini-2.5-flash", "gemini-1.5-pro", "gemini-1.5-flash"],
        "default_model": "gemini-2.5-pro"
      }
    }
  },
  "generation_settings": {
    "default_temperature": 0.8,
    "max_tokens": 4096,
    "rate_limit": {
      "requests": 10,
      "window": 60
    }
  }
